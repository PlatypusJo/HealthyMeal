<?xml version="1.0" encoding="utf-8" ?>
<Shell xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="HealthyMeal.AppShell"
             xmlns:views="clr-namespace:HealthyMeal.Views"
             Shell.NavBarIsVisible="False"
             Shell.TabBarIsVisible="False"
             Shell.FlyoutBehavior="Disabled">
    <ShellContent Route="AuthorizationPage" ContentTemplate="{DataTemplate views:AuthorizationPage}"/>
    <ShellContent Route="MainPage" ContentTemplate="{DataTemplate views:MainPage}"/>
</Shell>